    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "rc-queue-anim": "^1.2.2",
    "react": "^15.6.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^15.6.1",
    "react-fileupload": "^2.4.0",
    "react-router": "^2.6.1"
  },
